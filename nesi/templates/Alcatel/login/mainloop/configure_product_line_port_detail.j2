    no g992-5-a
    no g992-5-b
    no ansi-t1.424
    no etsi-ts
    no itu-g993-1
    no ieee-802.3ah
    no g992-5-aj
    no g992-5-am
    no g993-2-8a
    no g993-2-8b
    no g993-2-8c
    no g993-2-8d
    no g993-2-12a
    no g993-2-12b
    no g993-2-17a
    no g993-2-30a
    no g993-2-35b
