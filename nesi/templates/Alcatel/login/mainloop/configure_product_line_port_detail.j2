        no rtx-ds
        no rtx-us
        no tps-ds
        no tps-us
        no dra-ds
        no dra-us
    exit
    tca-line-threshold
        no enable
        no es-nearend # value=0
        no ses-nearend # value=0
        no uas-nearend # value=0
        no leftr-nearend # value=0
        no leftr-day-nearend # value=0
