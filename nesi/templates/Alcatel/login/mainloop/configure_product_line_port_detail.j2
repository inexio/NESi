    spectrum-profile-name {{ context.spectrum_profile_name|default(NONE, true) }}
    dpbo-profile-name {{ context.dpbo_profile_name|default(NONE, true) }}
    rtx-profile-name {{ context.rtx_profile_name|default(NONE, true) }}
    vect-profile-name {{ context.vect_profile_name|default(NONE, true) }}
