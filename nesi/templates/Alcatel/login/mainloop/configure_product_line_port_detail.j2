    no transfer-mode # value=system-default
    no imp-noise-sensor
    no vect-qln-mode # value=without-cancel
    no auto-switch
    no vect-fallback # value=auto
    no gf-tdd-profile # value=none
    no gf-dsrate-profile # value=none
    no gf-usrate-profile # value=none
    no gf-spec-profile # value=none
